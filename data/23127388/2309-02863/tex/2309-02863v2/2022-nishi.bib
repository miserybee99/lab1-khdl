}

@misc{ye_near-perfect_2023,
	title = {Near-{Perfect} {Logical} {Magic} {State} {Preparation} on a {Superconducting} {Quantum} {Processor}},
	url = {http://arxiv.org/abs/2305.15972},
	doi = {10.48550/arXiv.2305.15972},
	abstract = {Fault-tolerant quantum computing based on surface code has emerged as an attractive candidate for practical large-scale quantum computers to achieve robust noise resistance. To achieve universality, magic states preparation is a commonly approach for introducing non-Clifford gates. Here, we present a hardware-efficient and scalable protocol for arbitrary logical state preparation for the rotated surface code, and further experimentally implement it on the {\textbackslash}textit\{Zuchongzhi\} 2.1 superconducting quantum processor. An average of \$0.9943 {\textbackslash}pm 0.0002\$ logical fidelity at different logical states with distance-three is achieved. In particular, the magic state \$\{{\textbar}A{\textasciicircum}\{{\textbackslash}pi/4\}{\textbackslash}rangle\}\_L\$ is prepared with logical fidelity of \$0.9997 {\textbackslash}pm 0.0009 \$, which is significantly higher than the state distillation protocol threshold, 0.859, and even higher than the average fidelity of all physical gate operations. Our work provides a viable and efficient avenue for generating high-fidelity raw logical magic states, which is essential for realizing non-Clifford logical gates in the surface code.},
