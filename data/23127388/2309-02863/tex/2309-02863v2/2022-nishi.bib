	annote = {Comment: 26 pages, 25 figures + 6 pages, 5 figures in 3 appendices. v2: Added two appendices, revised and added references. See also https://errorcorrectionzoo.org/c/crystalline\_dynamic\_gen},
}

@misc{li_mapping_2023,
	title = {Mapping a topology-disorder phase diagram with a quantum simulator},
	url = {http://arxiv.org/abs/2301.12138},
	doi = {10.48550/arXiv.2301.12138},
	abstract = {The competition and interplay of topology and disorder has been one of the most famous topics in the field of condensed matter physics. In addition to the intuitive tendency to bring the system into a topologically trivial and localized phase, it has been discovered that disorder can also induce nontrivial topology and transport. To reveal rich and diverse phase structures, mapping phase diagrams plays an important role in both theoretical and experimental sides. Quantum simulation provides a prospective way to study the target model, explore the phase diagram and reveal the underlying mechanism. Thanks to the unprecedented controllability, superconducting quantum simulators have been introduced to investigate complex many-body physics and bring thought experiments into reality. To our best knowledge, the effort to map a phase diagram with a rich structure is still lacking. Here we report a systematic experimental study of the topology-disorder phase diagram with 32 qubits on a programmable analog quantum simulator. We implement one-dimensional (1D) disordered dimerized tight-binding models over a wide parameter range and observe diverse phases, including the topological Anderson insulator (TAI) and the inverse Anderson localization (IAL). Our experiment manifests the efficiency, accuracy and flexibility of the superconducting-circuit device and paves the way to the demonstration and understanding of many-body phenomena with noisy intermediate-scale quantum simulators.},
	urldate = {2023-02-28},
	publisher = {arXiv},
	author = {Li, Xue-Gang and Xu, Hui-Kai and Wang, Jun-Hua and Tang, Ling-Zhi and Zhang, Dan-Wei and Yang, Chu-Hong and Su, Tang and Wang, Chen-Lu and Mi, Zhen-Yu and Sun, Wei-Jie and Liang, Xue-Hui and Chen, Mo and Li, Cheng-Yao and Zhang, Ying-Shan and Linghu, Ke-Huan and Han, Jia-Xiu and Liu, Wei-Yang and Feng, Yu-Long and Liu, Pei and Xue, Guang-Ming and Zhang, Jing-Ning and Jin, Yi-Rong and Zhu, Shi-Liang and Yu, Hai-Feng and Xue, Qi-Kun},
	month = jan,
