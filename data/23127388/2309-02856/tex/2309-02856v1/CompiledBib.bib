@inproceedings{Abernethy2008Competing,
	Abstract = {We introduce an efficient algorithm for the prob-
lem of online linear optimization in the bandit set-
ting which achieves the optimal Oâ (âT ) regret.
The setting is a natural generalization of the non-
stochastic multi-armed bandit problem, and the ex-
istence of an efficient optimal algorithm has been
