@inproceedings{Strehl2006Incremental,
	Abstract = {Model-based learning algorithms have been
shown to use experience efficiently when
learning to solve Markov Decision Processes
(MDPs) with finite state and action spaces.
However, their high computational cost due
to repeatedly solving an internal model in-
hibits their use in large-scale problems. We propose a method based on real-time dynamic programming (RTDP) to speed up two model-based algorithms, RMAX and MBIE
(model-based interval estimation), resulting in computationally much faster algorithms with little loss compared to existing bounds. Specifically, our two new learning algorithms, RTDP-RMAX and RTDP-IE, have considerably smaller computational demands than RMAX and MBIE. We develop a general theoretical framework that allows us to prove that both are efficient learners in a PAC (probably approximately correct) sense. We also present an experimental evaluation of these new algorithms that helps quantify the tradeoff between computational and experience demands.},
	Author = {Strehl, Alexander L. and Li, Lihong and Littman, Michael L.},
	Booktitle = {Proceedings of the 22nd Conference on Uncertainty in Artificial Intelligence {(UAI)}},
	Citeulike-Article-Id = {3043312},
	Comment = {(private-note)Two main contributions 1) Gives a formal set of criteria for determining if a MDP RL algorithm has a polynomial sample complexity (on all but a certain number of samples it will give an action that is epsilon close to optimal). These criteria were later used in Strehl and Littman (NIPS Dec 2007) and by us in CORL (UAI 2008). 2) Provides modifications of Rmax and interval based estimation to greatly reduce the computational complexity. Specifically, only does a single Bellman backup, at the specific state and action taken, in both algorithm instances. This reduces the computational complexity from O(SKA) (where K is the number of non-zero prob states that can be reached in a single step) to O(K ln(A)) where the ln(A) comes from using a priority queue. Essentially this is going from S^2 -> S and also reducing the action factor as well. They prove this reduction in computational complexity still allows the sample complexity to be the same as Rmax (O(S^2 A/ ( eps^3 (1-gamma)^6) ) when one ignores log factors. Experimentally they find that MBIE/Rmac gets better performance with fewer samples, but they required a lot more computational than RTDP-Rmax/RTDP-IE. In bandit-style MDP tried restricting the number of samples used to estimate the models (small) vs allowing a large number. In the small case then some difference between 4 but not huge. But if allowed a large number (which presumably improved performance? would've been nice to see this fleshed out a little more) then RTDP-Rmax/RTDP-IE had a significantly smaller number of backups compared to MBIE/Rmax. It would've been nice to see a total time comparison here to get a sense of the trade off of sample complexity vs computational complexity. This will admittedly be platform specific but perhaps there is a platform argument to be made here: that if actions take a long time then maybe sample complexity dominates. Interesting that even though their dynamics model is changing, they can still get good results. In continuous-state case, restricting to one backup doesn't help a lot because still considering a next state transition to an infinite set of states...},
	Date-Added = {2013-11-17 23:24:29 +0000},
	Date-Modified = {2013-11-17 23:24:30 +0000},
	Keywords = {discrete-state, learning, mdp, model-based, reinforcement, theory},
	Posted-At = {2008-07-25 18:36:09},
	Priority = {0},
	Title = {Incremental Model-based Learners With Formal Learning-Time Guarantees},
	Year = {2006}}

@inproceedings{ngpegasus2000,
	Abstract = {We propose a new approach to the problem
of searching a space of policies for a Markov
decision process (MDP) or a partially observ-
able Markov decision process (POMDP), given
a model. Our approach is based on the following
observation: Any (PO)MDP can be transformed
into an ``equivalent'' POMDP in which all state
transitions (given the current state and action) are
deterministic. This reduces the general problem
of policy search to one in which we need only
consider POMDPs with deterministic transitions.
We give a natural way of estimating the value of
all policies in these transformed POMDPs. Pol-
icy search is then simply performed by searching
for a policy with high estimated value. We also
establish conditions under which our value esti-
mates will be good, recovering theoretical results
similar to those of Kearns, Mansour and Ng [7],
but with ``sample complexity'' bounds that have
only a polynomial rather than exponential depen-
dence on the horizon time. Our method applies
to arbitrary POMDPs, including ones with infinite state and action spaces. We also present
empirical results for our approach on a small discrete problem, and on a complex continuous state/continuous action problem involving learning to ride a bicycle.},
	Author = {Ng, Andrew Y. and Jordan, Michael},
	Booktitle = {Uncertainty in Artificial Intelligence {(UAI)}},
	Citeulike-Article-Id = {3023128},
	Comment = {(private-note)Policy search algorithm which has a sample complexity that is polynomial in the horizon time. Converts stochastic transitions into deterministic, by assuming that a random sequence of numbers is provided. Can obtain a set of random numbers and use these to evaluate a bunch of different potential policies (now a transition is deterministic for a given action since the random number determines the transition chosen). They optimize the policy on this simulated deterministic model since this will have the same optima as the original model. Have some theoretical results on the number of scenarios (random number strings) necessary and show this is polynomial in the desired quantities in order to obtain a V(pi) that is epsilon close to the true V(pi). In some ways can be thought of as a way to keep a compact trajectory tree. However, they still have to evaluate the transitions for each policy separately. Also, still have to find right policy over this deterministic class. In their grid world experiment they do this by exhaustively enumerating all potential policies (|A|^|Z|) and checking result for each. In their bike experiment (with continuous states and actions) they first convert the rep to a vector, restrict the action function to be sigmoid of the features, and use (well in 1 version) gradient ascent to optimize the weights. They also use reward shaping, all of which suggests to me that this is a tricky problem. So challenges seem - Still have to optimize over a policy class. This could be very expensive and/or lead to local optima. Their theoretical bounds (I believe) hold for comparing the optimal value of V that can be obtained over a set of m random number vectors vs the value of V for the true policy. I dont believe they take into account any error by not being able to find the global optimal value for the given policy class due to inadequate optimization. LSPI states: "Policy search methods typically make a large number of relatively small steps of gradient-based policy updates to a parameterized policy function." Kakade states: "Though we have a concise representation of the tree we still have the computational cost of computing a transition..." (for a particular MDP ) "with a randomly initialized policy, then the number of calls to the generative model needed to obtain a non-zero gradient is still exponential in T. Hence the number of transitions we must compute is still exponential in T... Essentially PEGASUS is a variance reduction mechanism not a means to solve exploration." "In general the lower bound for the sparse sampling algorithm suggests that ... A^T is unavoidable (since there could be some leaf of the tree that we must discover in order to find our good policy..." His comment seems to argue that the number of m could be small but the number of policies to evaluate could be huge and so effectively to find a good one we have still made an exponential number of calls to the transition function},
	Date-Added = {2013-11-17 23:24:29 +0000},
	Date-Modified = {2013-11-17 23:24:30 +0000},
	Keywords = {mdp, policy-search, polynomial, pomdp},
	Posted-At = {2008-07-20 20:55:10},
	Priority = {0},
	Title = {PEGASUS: A policy search method for large MDPs and POMDPs},
	Year = {2000}}

@article{Kearns2002Sparse,
	Abstract = {A critical issue for the application of Markov decision processes (MDPs) to realistic problems is how
the complexity of planning scales with the size of the MDP. In stochastic environments with very large or infinite
state spaces, traditional planning and reinforcement learning algorithms may be inapplicable, since their running
time typically grows linearly with the state space size in the worst case. In this paper we present a new algorithm
that, given only a generative model (a natural and common type of simulator) for an arbitrary MDP, performs
on-line, near-optimal planning with a per-state running time that has no dependence on the number of states. The
running time is exponential in the horizon time (which depends only on the discount factor Î³ and the desired
degree of approximation to the optimal policy). Our algorithm thus provides a different complexity trade-off than
classical algorithms such as value iteration---rather than scaling linearly in both horizon time and state space size,
our running time trades an exponential dependence on the former in exchange for no dependence on the latter.
Our algorithm is based on the idea of sparse sampling. We prove that a randomly sampled look-ahead tree that
covers only a vanishing fraction of the full look-ahead tree nevertheless suffices to compute near-optimal actions
from any state of an MDP. Practical implementations of the algorithm are discussed, and we draw ties to our related
recent results on finding a near-best strategy from a given class of strategies in very large partially observable
MDPs (Kearns, Mansour, \& Ng. Neural information processing systems 13, to appear).},
	Author = {Kearns, Michael and Mansour, Yishay and Ng, Andrew},
	Citeulike-Article-Id = {3023040},
	Comment = {(private-note)Main result: that given a generative model for a MDP M, the authors provide an algorithm A which for any state returns an action a such that the policy implemented by A is near optimal in M, and that the running time required for A to compute an action a for any state has no dependence on the number of state in M. The authors assume that rewards are bounded in absolute value by Rmax. Algorithm: given s and epsilon > 0, then will output an action by sampling C next states for each potential action a, and repeating this process for each next state until the total depth of the tree is H. The running time is an exponential function of the horizon time. or O( (1/epsilon)^O( (1/epsilon)*log (1/ (epsilon ) ) or exponential in (1/(1-gamma)). They provide expressions for C and H: algorithm is just a func of Rmac, epsilon and gamma (discount). Prove that any algorithm with only access to a generative model and algorithm like above can, in the worst case for a particular MDP, require at least O( (1/epsilon)^(1/log(1/gamma)) calls to the generator in order to get an epsilon close answer. I didn't fully understand their comparison of the two bounds. Would be great to have a running time that is only polynomial in all the relevant qualities. Should look up their other algorithm.},
	Date-Added = {2013-11-17 23:24:29 +0000},
	Date-Modified = {2013-11-17 23:24:30 +0000},
	Editor = {Kaelbling, Leslie},
	Journal = {Machine Learning},
	Keywords = {large, mdp, sampling},
	Pages = {193--208},
	Posted-At = {2008-07-20 18:18:02},
	Priority = {0},
	Title = {A Sparse Sampling Algorithm for Near-Optimal Planning in Large Markov Decision Processes},
	Volume = {49},
	Year = {2002}}

@inproceedings{Amato2008Optimizing,
	Abstract = {In this paper, we discuss a new approach that represents
POMDP policies as finite-state controllers and formulates
the optimal policy of a desired size as a nonlinear pro-
gram (NLP). This new representation allows a wide range
of powerful nonlinear programming algorithms to be used
to solve POMDPs. Although solving the NLP optimally
is often intractable, the results we obtain using an off-the-
shelf optimization method are competitive with state-of-the-
art POMDP algorithms. Our approach is simple to imple-
ment and it opens up promising research directions for solv-
ing POMDPs using nonlinear programming methods.},
	Author = {Amato, Christopher and Bernstein, Daniel S. and Zilberstein, Shlomo},
	Booktitle = {Workshop on Advancements in POMDP Solvers (at AAAI)},
	Citeulike-Article-Id = {3008972},
	Comment = {(private-note)Optimize a fixed size stochastic controller use nonlinear program find globally optimal fixed size stochastic controller doesn't do nearly as well as Perseus/HSVI on some problems in machine and aloha domain did better when the discount factor was very close to 1 hallway they did better than HSVI},
	Date-Added = {2013-11-17 23:24:29 +0000},
	Date-Modified = {2013-11-17 23:24:30 +0000},
	Keywords = {finite-state-controller, nonlinear, pomdp, programming},
	Posted-At = {2008-07-16 18:02:16},
	Priority = {2},
	Title = {Optimizing Fixed-Size Stochastic Controllers for POMDPs},
	Year = {2008}}

@article{gmapping,
	Abstract = {Recently, Rao-Blackwellized particle filters (RBPF) have been introduced as an effective means to solve the simultaneous localization and mapping problem. This approach uses a particle filter in which each particle carries an individual map of the environment. Accordingly, a key question is how to reduce the number of particles. In this paper, we present adaptive techniques for reducing this number in a RBPF for learning grid maps. We propose an approach to compute an accurate proposal distribution, taking into account not only the movement of the robot, but also the most recent observation. This drastically decreases the uncertainty about the robot's pose in the prediction step of the filter. Furthermore, we present an approach to selectively carry out resampling operations, which seriously reduces the problem of particle depletion. Experimental results carried out with real mobile robots in large-scale indoor, as well as outdoor, environments illustrate the advantages of our methods over previous approaches},
	Author = {Grisetti, G. and Stachniss, C. and Burgard, W.},
	Booktitle = {IEEE TRO},
	Citeulike-Article-Id = {3501823},
	Date-Added = {2013-11-17 23:24:29 +0000},
	Date-Modified = {2013-11-17 23:24:30 +0000},
	Doi = {http://dx.doi.org/10.1109/TRO.2006.889486},
	Journal = {IEEE Transactions on Robotics},
	Keywords = {filter, particle, slam},
	Number = {1},
	Pages = {34--46},
	Posted-At = {2008-11-10 21:11:42},
	Priority = {2},
	Title = {Improved Techniques for Grid Mapping With Rao-Blackwellized Particle Filters},
	Volume = {23},
	Year = {2007},
	Bdsk-Url-1 = {http://dx.doi.org/10.1109/TRO.2006.889486}}

@inproceedings{Toussaint2008Hierarchical,
	Abstract = {Planning can often be simplified by decomposing the task into smaller tasks arranged
hierarchically. Charlin et al. [4] recently
showed that the hierarchy discovery problem
can be framed as a non-convex optimization
problem. However, the inherent computa-
tional difficulty of solving such an optimization problem makes it hard to scale to real-world problems. In another line of research, Toussaint et al. [18] developed a method
to solve planning problems by maximum-
likelihood estimation. In this paper, we show how the hierarchy discovery problem in partially observable domains can be tackled using a similar maximum likelihood approach. Our technique first transforms the problem into a dynamic Bayesian network through which a hierarchical structure can naturally be discovered while optimizing the policy. Experimental results demonstrate that this approach scales better than previous techniques based on non-convex optimization.},
	Author = {Toussaint, Marc and Charlin, Laurent and Poupart, Pascal},
	Booktitle = {Uncertainty in Artificial Intelligence {(UAI)}},
	Citeulike-Article-Id = {3008816},
	Comment = {(private-note)Notes from UAI: solve POMDP using hierarchical finite state controller Charlin, Poupart et al did hierarchy discovery for POMDPs using quadratic optimization Known that can do linear time inerence in hierarchical HMMs (using exit states) mixture of processes that emits reward at final time slice EM: maximium likelihood corresponds to maximum expcted return only single forward and backward pass needed to do all DBNs Prefix the number of nodes per level (in the FSC) faster than their old approach \& similar in quality to HSVI Hierarchical controller better than flat (think flat overfit) factored controller better than hierarchical One issue is the EM only finds local min They have code available on Marc's webpage Questions If add fixed poitn to reward then it will rescale likelihood and change EM. They said they handle this using a special M step Stuart: why not do LP directly Poupart: can't do on original MDP, not linear. So instead go from quad -> graph -> LP My original notes: This paper takes a useful step towards autonomously finding hierarchical controllers to solve POMDP problems. Hierarchical controllers have been previously found to improve the efficiency of planning. In this paper the authors present an automatic way to induce the parameters of a hierarchical controller when given the number of levels and the number of nodes per level of the controller. Prior work by Toussaint demonstrated how one could represent the controller for a POMDP as a DBN with a special reward formulation, and solve for a good controller by using EM (which involved modifying the policy parameters). Other past work by Theocharous demonstrated how to convert a hierarchical POMDP to a DBN. In the current paper the authors unite these two ideas to show that a hierarchical finite state controller for a POMDP can be converted into a DBN, and then solved using techniques similar to Toussaint. This is an interesting paper and the promise of automatically finding the hierarchical structure of a controller is an important one. However, the paper would be much improved if the experimentalresults had been stronger. In particular, it would be helpful for the authors to compare to either state of the art POMDP solvers (such as HSVI) on the presented problems to demonstrate the benefit of hierarchical finite state controllers, or to solve a very large problem that could not be solved by existing techniques that lack hierarchy. In addition, the experimental results indicate that choosing the right number of nodes for each layer is critical in order for a good hierarchical controller to be found: though this is perhaps outside the scope of this paper, it is clear that structure search is a very interesting and key piece of future work. EM only finds a local minima, and this seems an important concern for some of these approaches. The error bars in table 2 are quite large, suggesting that often solutions vary significantly in their quality. Do the authors have ideas about how to tackle this issue further? Smaller points: - I believe that the authors should refer to Paskin and Murphy's NIPS 2001 paper for the citation about converting hierarchical HMM to DBNs rather than Theocharous et al[2004]. Theocharous et al. showed how to convert hierarchical POMDPs to DBNs (including a representation for the action node) but Paskin and Murphy were to my knowledge those responsible for first showing that HHMMs could be converted to DBNs (which was built on by Theocharous et al.). Therefore perhaps the authors should change 3.2 to refer to HPOMDPs rather than HHMMs, or else cite the alternate paper. - In section 4.1 there is no need to repeat all of the definitions for when the E variables are removed: just mention this initially and then do all definitions for the E variable removed. - How sensitive is the method to initialization? In particular, if the parameters c1,c2 and c3 are changed in section 4.2.1, does this significantly impact the results? - The greedy M-step (section 4.2.3) was very confusing. f did not appear to be defined: if it is defined earlier, please remind the reader, or else give an more clear example of the type of function f represents. A more detailed discussion would be very helpful in this section. Also I'm afraid that the steps taken to try to improve the chance of monotonic improvement were also not clear: what does adding c do? - If figure 4 could be moved to the same page where it is discussed, that would be helpful - It is hard to read the figures in the right hand side of table 2: perhaps the numbers could be made larger, or only a few could be presented. - A small table to compare the complexity of the various approaches, rather than discussing them in the text, would make it clearer to see the differences.},
	Date-Added = {2013-11-17 23:24:29 +0000},
	Date-Modified = {2013-11-17 23:24:30 +0000},
	Keywords = {expectation-maximization, finite-state-controller, hierarchy, inference, pomdp},
	Posted-At = {2008-07-16 17:31:16},
	Priority = {0},
	Title = {Hierarchical POMDP Controller Optimization by Likelihood Maximization},
	Year = {2008}}

@inproceedings{Kveton2008Partitioned,
	Abstract = {Approximate linear programming (ALP) is an ef-
ficient approach to solving large factored Markov
decision processes (MDPs). The main idea of the
method is to approximate the optimal value func-
tion by a set of basis functions and optimize their
weights by linear programming (LP). This paper
proposes a new ALP approximation. Comparing
to the standard ALP formulation, we decompose
the constraint space into a set of low-dimensional
spaces. This structure allows for solving the new
LP efficiently. In particular, the constraints of the
LP can be satisfied in a compact form without an
exponential dependence on the treewidth of ALP
constraints. We study both practical and theoret-
ical aspects of the proposed approach. Moreover,
we demonstrate its scale-up potential on an MDP
with more than 2100 states.},
	Author = {Kveton, Branislav and Hauskrecht, Milos},
	Booktitle = {Proceedings of the 24th Conference on Uncertainty in Artificial Intelligence {(UAI)}},
	Citeulike-Article-Id = {3008804},
	Comment = {(private-note)Discrete state space Constraint space of ALP represented as a cost network Descompose the space into a set of subspaces (with smaller treewidth) When decomposition is convex solution in PALP is feasible in corresponding ALP (but suboptimal) Can be viewed as solving K MDPs with overlapping state and action spaces and shared value function How to generate the best partition is still open question: need to preserve key dependencies (in their current work just use immediate neighbourhood I believe) PALP upper bounds the min L1 norm error Something is poly vs exponential :) Better than monte carlo constraint sampling in their future work would like to investigate using technique for doing inference when there is a large tree width},
	Date-Added = {2013-11-17 23:24:29 +0000},
