@inproceedings{Castro2007Using,
	Abstract = {A key problem in reinforcement learning is finding a good balance between the need to explore the environment and the need to gain rewards by exploiting existing knowledge. Much research has been devoted to this topic, and many of the proposed methods are aimed simply at ensuring that enough samples are gathered to estimate the value function well. In contrast, in 1959 Bellman proposed constructing a representation in which the states of the original system are paired with knowledge about the current model. Hence, knowledge about the possible Markov models of the environment is represented and maintained explicitly. Unfortunately, this approach is intractable except for bandit problems (where it gives rise to Gittins indices, an optimal exploration method). In this paper, we explore ideas for making this method computationally tractable. We maintain a model of the environment as a Markov Decision Process. We sample finite-length trajectories from the infinite tree using ideas based on sparse sampling. Finding the values of the nodes of this sparse subtree can then be expressed as an optimization problem, which we solve using Linear Programming. We illustrate this approach on a few domains and compare it with other exploration algorithms.},
	Author = {Castro, Pablo and Precup, Doina},
	Booktitle = {20th International Joint Conference on Artificial Intelligence {(IJCAI)}},
	Citeulike-Article-Id = {3142255},
	Comment = {(private-note)Take a Bayesian approach to RL: incorporate model parameters in state. Represent as a beta distribution, fraction of times made different transitions. Hyperstate is the state, the current dynamics model counts, and the current reward model estimates. If take an action and transition to new state j, know how to update dynamics model and reward model counts. hyper-state MDP is an infinite tree of states, can express bellman equation over this. They maintain state-action values of original MDP using q-learning. Also compute values of hyperstates using linear programming. Linear program is as large as number of hyperstates considered. So they construct a sampled tree: sample using current estimates of action values to decide what to do, at each node use corresponding estimates of dynamics model to sample next node in trajectory, which was expanded to certain horizon. As go forward, in order to sample next action, either use value according to prior LP (if have that) for next hyperstate, or use state-action value estimate of MDP state. After build hyper tree then solve using linear programming. Have to decide whether to redo LP after each sample or only incrementally. Compared to q-learning and Wang's algorithm (similar). q-learning outperformed in some cases. In a 2x4 grid world they outperformed both: seemed strange q-learning was quite so bad, but may depend on alpha and epsilon value. would neat to try running this but probably not wort it. Only run on very small problems. Have optimal criteria for exploration/exploitation but only approximately solve. no theoretical guarantees. don't compare to Rmax. Say that some prior PAC work uses only myopic strategies, don't consider long term effect actions have on reward. key comparison to CORL: approximate solution for optimal criteria, not clear can scale to larger problems (or continuous problems), no theoretical analysis of how close to optimal strategy.},
	Date-Added = {2013-11-17 23:24:29 +0000},
	Date-Modified = {2013-11-17 23:24:30 +0000},
	Keywords = {bayesian, learning, mdps, reinforcement, rollouts, sampling, sparse},
	Pages = {2437--2442},
	Posted-At = {2008-08-20 23:08:19},
	Priority = {0},
	Title = {Using linear programming for {Bayesian} exploration in {Markov} Decision Processes},
	Year = {2007}}

@inproceedings{jongsara2007,
	Abstract = {Reinforcement learning promises a generic method for adapting agents to arbitrary tasks in arbitrary stochastic environments, but applying it to new real-world problems remains difficult, a few impressive success stories notwithstanding. Most interesting agent-environment systems have large state spaces, so performance depends crucially on efficient generalization from a small amount of experience. Current algorithms rely on model-free function approximation, which estimates the long-term values of states and actions directly from data and assumes that actions have similar values in similar states. This paper proposes model-based function approximation, which combines two forms of generalization by assuming that in addition to having similar values in similar states, actions also have similar effects. For one family of generalization schemes known as averagers, computation of an approximate value function from an approximate model is shown to be equivalent to the computation of the exact value function for a finite model derived from data. This derivation both integrates two independent sources of generalization and permits the extension of model-based techniques developed for finite problems. Preliminary experiments with a novel algorithm, AMBI (Approximate Models Based on Instances), demonstrate that this approach yields faster learning on some standard benchmark problems than many contemporary algorithms.},
	Author = {Jong, Nicholas K. and Stone, Peter},
	Booktitle = {Sixth International Conference on Autonomous Agents and Multiagent Systems (AAMAS)},
	Citeulike-Article-Id = {3141345},
	Comment = {(private-note)Creates a model approximation of continuous dynamics using an instance based approach: assumes nearby states have similar dynamics. Handles both absolute and relative dynamics. Combines this with fitted value iteration to create a complete model-based reinforcement learning. If there aren't enough samples "near" a new point, then use an optimistic approach (assume transition to high reward). This is true both for dynamics approximation (averager) and for value function approximator. They use a Gaussian weighting as the averager, but restrict it in practice to nearest 100 neighbours. Got good experimental results on mountain car and puddle world compared to some prior approaches. Using relative dynamics was important. Not clear how careful have to tune widths of Gaussians (basically how much can generalize between states). In theory the algorithm slows down as they get more instances, but they cap the number used to approximate the dynamics and then becomes a K nearest neighbours problem which can k log D time I think using a KD tree. No theoretical guarantees on how good result is or how many samples are needed into get good results. Know that solution will converge. As dimensionality of problem increases exponentially more data is required to explore each neighbourhood of state space. In contrast our's grows polynomially in the state space. key comparison to CORL: continuous-state model based approach which uses an instance based approach. doesn't provide theoretical guarantees on quality of approximation and amount of data scales exponentially with state space dimension},
	Date-Added = {2013-11-17 23:24:29 +0000},
	Date-Modified = {2013-11-17 23:24:30 +0000},
	Keywords = {approximation, continuous-state, function, learning, mdp, reinforcement},
	Posted-At = {2008-08-20 19:10:17},
