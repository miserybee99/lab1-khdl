@inproceedings{Kim2008Exploiting,
	Abstract = {We extend the model minimization technique for partially observable Markov decision processes (POMDPs) to handle symmetries in the joint space of states, actions, and observations. The POMDP symmetry we define in this paper cannot be handled by the model minimization techniques previously published in the literature. We formulate the problem of finding the symmetries as a graph automorphism (GA) problem, and although not yet known to be tractable, we experimentally show that the sparseness of the graph representing
the POMDP allows us to quickly find symmetries. We show how the symmetries in POMDPs can be exploited for speeding up point-based algorithms. We experimentally demonstrate the effectiveness of our approach.},
	Author = {Kim, Kee-Eung},
	Booktitle = {Association for the Advancement of Artificial Intelligence},
	Citeulike-Article-Id = {2961391},
	Comment = {(private-note)Looks for symmetries or automorphisms in POMDP state-action-observation space. Creates a graph of transitions, rewards, observations and states (for a discrete-state/action/observation POMDP) and computes all the automorphisms in this graph (which has a number of edges of order s^2*a+s*z*a). The automorphism (of switching around the states) of all belief points and alpha-vectors are also in feasible good elements of the POMDP. in PBVI consider belief point with farthest distance: now they exclude redundant belief points by calc distance between all possible symmetric (under an automorphism) images of belief points. Also modify backup to include all symmetric images of alpha-vectors into alpha-vector set. Should check if automorphism is a mapping to self (b=f(b)) so don't add in additional alpha vectors here. Collected all beliefs then ran. Got a significant speedup compared to using standard PBVI on full set of beliefs (ones used in symmetric PBVI plus the symmetric ones of those). Did on fairly small problems so not sure how expensive it is to compute automorphisms as scale up to large state, action, observ state spaces, but I imagine it's not too bad. Could apply to other PB techniques. Think they can also use for factored or multi-agent techniques. Also layers on top of existing model minimization techniques which is nice. Not clear how to think about continuous symmetries: do these exist?},
	Date-Added = {2013-11-17 23:24:29 +0000},
	Date-Modified = {2013-11-17 23:24:30 +0000},
	Keywords = {automorphism, pomdps},
	Posted-At = {2008-07-03 22:52:11},
	Priority = {0},
	Title = {Exploiting Symmetries in POMDPs for Point-Based Algorithms},
	Year = {2008}}

@inproceedings{Sim2008Symbolic,
	Abstract = {We propose Symbolic heuristic search value iteration
(Symbolic HSVI) algorithm, which extends the heuristic search value iteration (HSVI) algorithm in order to handle factored partially observable Markov decision processes (factored POMDPs). The idea is to use algebraic decision diagrams (ADDs) for compactly representing the problem itself and all the relevant intermediate computation results in the algorithm. We leverage Symbolic Perseus for computing the lower bound of the optimal value function using ADD operators, and provide a novel ADD-based procedure for computing the upper bound. Experiments on a number of standard factored POMDP problems show that we can achieve an order of magnitude improvement in performance over previously proposed algorithms.},
	Author = {Sim, Hyeong S. and Kim, Kee-Eung and Kim, Jin H. and Chang, Du-Seong and Koo, Myoung-Wan},
	Booktitle = {Association for the Advancement of Artificial Intelligence},
	Citeulike-Article-Id = {2961378},
	Comment = {(private-note)Only skimmed Use ADDs to compute upper and lower HSVI bounds of factored POMDP without fully enumerating states \& observations. Use sawtooh upper bound approximation and factored belief states. Represents belief point as product of independent marginals: wonder if this is similar to Brian Milch et al's representation (presumably the authors here are not normalizing thoe belief points explicitly) Got a significant execution improvement over HSVI and symbolic Perseus (not clear why symbolic Perseus was often so close to HSVI or occasionally worse). Could we do this for FSVI? Seems like, and might lead to further significant improvement.},
	Date-Added = {2013-11-17 23:24:29 +0000},
	Date-Modified = {2013-11-17 23:24:30 +0000},
	Keywords = {adds, algebraic-decision-diagrams, factor, hsvi, pomdps},
	Posted-At = {2008-07-03 22:37:43},
	Priority = {0},
	Title = {Symbolic Heuristic Search Value Iteration for Factored POMDPs},
	Year = {2008}}

@inproceedings{Mausam2007Hybridized,
	Abstract = {Markov Decision Processes are a powerful frame-
work for planning under uncertainty, but current algorithms have difficulties scaling to large problems.  We present a novel probabilistic planner based on the notion of hybridizing two algorithms. In particular, we hybridize GP T, an exact MDP solver, with MBP, a planner that plans using a qualitative (non-deterministic) model of uncertainty. Whereas exact MDP solvers produce optimal solutions, qualitative planners sacrifice optimality to achieve speed and high scalability. Our hybridized planner, HYB-PL AN, is able to obtain the best of both techniques --- speed, quality and scalability. Moreover, HYB - PL AN has excellent anytime properties and makes effective use of available time and memory.},
	Author = {Mausam and Bertoli, Piergiorgio and Weld, Daniel S.},
	Booktitle = {20th International Joint Conference on Artificial Intelligence {(IJCAI)}},
	Citeulike-Article-Id = {2953094},
	Comment = {(private-note)Authors introduce HYB-Plan: a hybrid planner that combines GPT (exact MDP solver with MBP which plans with a qualitative model of uncertainty. Intuition is that qualitative solvers scale to larger problems but have poorer solutions: combining to give best of both. HYB-Plan anytime algorithm, converges to desired optimality bound, and reduces to optimal planning given infinite time and memory. Use labeled RTDP. RTDP simulates greedy policy along single trace of execution and updates values of visited states with Bellman backups. RTDP trial is a path starting at initial state and ending when goal reached/number of steps=N. Trials repeated until convergence. Common states updated frequently and no time wasted on unreachable states. Full convergence is slow and not guaranteed. Labeled RTDP. Label states as solved (when V(s) converges). Back prop this info from goal state and stop when s0 labeled. Guaranteed to terminate and converge to V* if original V admissable for states on pi*?). Not good for undiscounted absorbing goals since intermediate policies may have an absorbing cycle (leads to inf cost to get to goal). So not anytime for such problems Model Based Planner. Uses binary decision diagrams. Algorithms within MBPs handle qual non-det problems. Rep policy as BDD and construct by backward chaining from goal. Iteratively regress from solution state set (current policy): first allow any loops introduced by backward step,, then remove loops for which no chance of goal achievement. Search stops when policy includes s0 or reach fixed point. Hybrid: Run GPT for a bit. Assign actions using action from GPT if state is labeled or has had N updates, else use MBP. remove cycles . Can continue to run GPT if desired. Get an error bound of [Jhyb(s0) - Jgpt(s0) ]/Jgpt(s0). Produces only proper policies. Implementation: compute MBP for a particular s on demand. Don't eval pi\_hyb by solving set of eqns: approximate by avg repeated simulations of policy from start stat. If traj > max length, assume absorb cycle and break by recursively assigning action of pi\_mbp for state in cycle. rm property of reaching goal with prob 1 since my not simulate potential cycle/ Some problems that may benefit from 1) POMDPs: use GPT or PBVI and MBP, BBSP or POND <--- look these up 2) deterministic over subscription planning 3) probabilistic planning with continuous resources: hybrid AO* algorithm on abstract state space, could combine w/alg that solves prob assuming determ consumption using mean <-- did they do this later 4) concurrent MDP: MDP w/factored action rep probs w/concurrency. Could be used w/equiv single action MDPs My questions - how well does it work when no single goal area, just acting over long horizon (can MBP handle?) - Should look p how MBPs work - Neither informs the other: could be interesting to bootstrap solutions though that would lose performance guarantee},
	Date-Added = {2013-11-17 23:24:29 +0000},
	Date-Modified = {2013-11-17 23:24:30 +0000},
	Keywords = {approximate, decision, markov, mdp, online, planner, process, solution},
	Posted-At = {2008-07-02 19:57:31},
	Priority = {0},
	Title = {A Hybridized Planner for Stochastic Domains},
	Year = {2007}}

@inproceedings{Shani2007Forward,
	Abstract = {Recent scaling up of POMDP solvers towards re-
alistic applications is largely due to point-based
methods which quickly converge to an approximate
solution for medium-sized problems. Of this family
HSVI, which uses trial-based asynchronous value
iteration, can handle the largest domains. In this
paper we suggest a new algorithm, FSVI, that uses
the underlying MDP to traverse the belief space to-
wards rewards, finding sequences of useful back-
ups, and show how it scales up better than HSVI on
larger benchmarks.},
	Author = {Shani, Guy and Brafman, Ronen I. and Shimony, Solomon E.},
	Booktitle = {20th International Joint Conference on Artificial Intelligence {(IJCAI)}},
	Citeulike-Article-Id = {2953082},
	Comment = {(private-note)A very fast heurstic point based POMDP solver. Uses as input solution to the underlying MDP. Gathers beliefs by simulating a trajectory, choosing actions according to MDP solution for s. Uses the beliefs along these trajectories to do point based backups at. Since doing PB backup still get 1 step information gathering (since info gathering may be best for a belief). Can't handle when domains need long series of information gathering. However, turns out that most canonical POMDP problems, including rock sample, only need short information gathering. FSVI is very fast here, looks like over an order of magnitude faster than HSVI. However, no theoretical guarantees on solution (which HSVI provides).},
