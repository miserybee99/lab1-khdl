	keywords = {Pandemics, Humans, {COVID}-19, {SARS}-{CoV}-2, {BCG} Vaccine, Immunity, Innate, {BCG}, trained immunity, Bacillus, glucosaminylmuramyl dipeptide, muramylpeptides, {NOD}2, non-specific prophylaxis, respiratory infections, Russia},
