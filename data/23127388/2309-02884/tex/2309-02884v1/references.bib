	keywords = {Computer Science - Artificial Intelligence, Computer Science - Computation and Language, Computer Science - Computers and Society, I.2.7, I.2.m},
	file = {arXiv Fulltext PDF:/Users/Supun/Zotero/storage/WIBPX3YS/Weber-Wulff et al. - 2023 - Testing of Detection Tools for AI-Generated Text.pdf:application/pdf;arXiv.org Snapshot:/Users/Supun/Zotero/storage/YKNSGULT/2306.html:text/html},
