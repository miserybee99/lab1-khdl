	doi = {10.48550/arXiv.2005.14165},
	keywords = {Computer Science - Computation and Language},
	file = {arXiv Fulltext PDF:/Users/Supun/Zotero/storage/2E4GE6E2/Brown et al. - 2020 - Language Models are Few-Shot Learners.pdf:application/pdf;arXiv.org Snapshot:/Users/Supun/Zotero/storage/WKC6XLVD/2005.html:text/html},
