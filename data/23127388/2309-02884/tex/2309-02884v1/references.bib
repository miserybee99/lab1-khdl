	file = {arXiv.org Snapshot:/Users/Supun/Zotero/storage/C6X9WQJS/2108.html:text/html;Full Text PDF:/Users/Supun/Zotero/storage/SE8C9CFW/Bommasani et al. - 2022 - On the Opportunities and Risks of Foundation Model.pdf:application/pdf},
