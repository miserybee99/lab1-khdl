	keywords = {Computer Science - Computation and Language, Computer Science - Machine Learning},
	file = {arXiv Fulltext PDF:/Users/Supun/Zotero/storage/JB9LCNIL/Jiang et al. - 2023 - Active Retrieval Augmented Generation.pdf:application/pdf;arXiv.org Snapshot:/Users/Supun/Zotero/storage/2XHBLZPX/2305.html:text/html},
