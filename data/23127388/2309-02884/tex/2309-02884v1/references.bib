	keywords = {Pandemics, Coronavirus, Humans, {COVID}-19, {SARS}-{CoV}-2, {BCG} Vaccine, Immunity, Innate, {BCG}, Trained immunity, Case-Control Studies, Innate immunity, Canada, Clinical Trials, Phase {III} as Topic},
