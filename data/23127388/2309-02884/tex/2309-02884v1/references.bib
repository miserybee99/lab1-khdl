	date = {2020-10-27},
	pmid = {33110184},
	pmcid = {PMC7591473},
	keywords = {Pandemics, Humans, {COVID}-19, {BCG} Vaccine, Coronavirus Infections, Pneumonia, Viral, Tuberculosis, Mass Vaccination, Correlation of Data, Health Policy},
