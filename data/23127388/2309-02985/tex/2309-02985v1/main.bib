@article{mijalkovic_traceability_2022,
	title = {Traceability {Analysis} of {Wireless} {Sensor} {Network} based {IoT} {Systems}},
	abstract = {In recent years, various concepts, methodologies, and tools have emerged to tackle the complexity of multi-paradigm systems using model-driven engineering (MDE) to improve the usability, precision and automation of these systems. Previously, we proposed a process model for the development of Wireless Sensor Networkbased IoT (Internet of Things) Systems. Explicit modelling of the process paves the way for automation, analysis and optimization of the engineering process. In this exemplar paper, we use and extend a model traceability framework to orchestrate and analyze WSN-based IoT Systems. Traceability information is generated by automatically enacting the WSN-based IoT development process, thus enabling origin tracking and change impact analysis of these systems.},
	language = {en},
	author = {Mijalkovic, Marko and Mustafiz, Sadaf and Karaduman, Burak},
	year = {2022},
	file = {Mijalkovic et al. - 2022 - Traceability Analysis of Wireless Sensor Network b.pdf:C\:\\Users\\claudio\\Zotero\\storage\\T7R6NX88\\Mijalkovic et al. - 2022 - Traceability Analysis of Wireless Sensor Network b.pdf:application/pdf},
}
